{
  "name": "tiddlywiki-eu-meetup-2016",
  "version": "0.0.1",
  "description": "Auto deploy the TW EU eetup homepage",
  "main": "index.js",
  "scripts": {
    "start": "tiddlywiki wiki-server/ --server 8080 $:/core/save/all text/plain text/html \"\" \"\" 0.0.0.0  # TODO there may be a problem in windows?!?!?",
    "serve": "npm start",
    "test": "echo ---- no tests atm ----",
    "prebuild": "rimraf wiki/output/*",
    "build": "tiddlywiki wiki --verbose --build",
    "prestage": "npm run build",
    "stage": "npm run stage:mkdir && npm run stage:rm && npm run stage:cp",
    "stage:mkdir": "mkdir -p ../jermolene.github.io/tiddlywiki-eu-meetup-2016",
    "stage:rm": "rimraf ../jermolene.github.io/tiddlywiki-eu-meetup-2016/*",
    "stage:cp": "cp -R wiki/output/* ../jermolene.github.io/tiddlywiki-eu-meetup-2016"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jermolene/tiddlywiki-eu-meetup-2016.git"
  },
  "keywords": [
    "tiddlywiki",
    "eu",
    "meetup",
    "2016"
  ],
  "author": "Jeremy Ruston",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Jermolene/tiddlywiki-eu-meetup-2016/issues"
  },
  "homepage": "http://tiddlywiki.com/tiddlywiki-eu-meetup-2016/index.html",
  "dependencies": {
    "tiddlywiki": "^5.1.12"
  },
  "devDependencies": {
    "rimraf": "latest"
  }

}
